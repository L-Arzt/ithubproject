// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model timetable {
  id           Int      @id @default(autoincrement())
  numberLesson Int
  data         DateTime
  classroom    Int
  discipline   String
  group        String
  teacher      String
  edited       Boolean
  booked       Boolean
}
